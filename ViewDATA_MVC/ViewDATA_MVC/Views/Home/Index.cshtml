
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<ul>
    @{
        foreach (string i in (string[])ViewData["NameList"])
        {
            <li>@i</li>
        }
    }
</ul>
<ul>
    @{
        foreach (string i in (List<string>)ViewData["Sports"])
        {
            <li>@i</li>
        }
    }
</ul>
@{
    var a = (ViewDATA_MVC.Models.Student)ViewData["Student"];
}
<ul>
    <li>@a.StdID</li>
    <li>@a.StdName</li>
    <li>@a.StdClass</li>
</ul>

